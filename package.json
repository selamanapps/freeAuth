{
  "name": "freeauth",
  "version": "1.0.0",
  "description": "Free authentication service based on Telegram - Production Ready",
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "start:prod": "pm2 start src/server/index.js --name FreeAuth",
    "dev": "NODE_ENV=development node src/server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "docker:build": "docker build -t freeauth .",
    "docker:run": "docker run -p 3000:3000 --env-file .env freeauth"
  },
  "keywords": [
    "authentication",
    "telegram",
    "auth",
    "security",
    "free-tier"
  ],
  "author": "Chibo",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "telegraf": "^4.16.3",
    "uuid": "^10.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "cost-of-modules": "^1.0.1",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/freeauth.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/freeauth/issues"
  },
  "homepage": "https://github.com/yourusername/freeauth#readme"
}