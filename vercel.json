{
    "version": 2,
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/",
            "dest": "/public/index.html"
        },
        {
            "src": "/support",
            "dest": "/public/support.html"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/public/404.html",
            "status": 404
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "github": {
        "enabled": true,
        "silent": true
    }
}